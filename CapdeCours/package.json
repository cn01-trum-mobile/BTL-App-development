{
  "name": "demo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:onboarding": "jest Onboarding-test.tsx --coverage --watchAll=false",
    "test:home": "jest home-test.tsx --coverage --watchAll=false",
    "generate:api": "openapi --input http://35.222.243.209:80/openapi.json --output src/api --client fetch --useUnionTypes"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ],
    "testMatch": [
      "**/__tests__/*.ts?(x)"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx}",
      "components/**/*.{ts,tsx}",
      "utils/**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/*.config.{js,ts}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/.expo/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "coverageDirectory": "coverage"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.19",
    "@sentry/react-native": "^7.7.0",
    "@testing-library/react-native": "^13.3.3",
    "date-fns": "^4.1.0",
    "expo": "54.0.23",
    "expo-build-properties": "~1.0.10",
    "expo-calendar": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.16",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-manipulator": "^14.0.8",
    "expo-linking": "~8.0.8",
    "expo-media-library": "~17.0.5",
    "expo-navigation-bar": "~5.0.9",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.9",
    "lucide-react-native": "^0.548.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "^8.0.0",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "5.6.0",
    "react-native-screens": "~4.18.0",
    "react-native-swiper": "^1.6.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.13",
    "openapi-typescript-codegen": "^0.30.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
